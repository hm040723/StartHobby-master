{
  "version": 2,
  "functions": {
    "api/server/index.js": {
      "runtime": "vercel-node@3.0.0"
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "server/index.js" },
    { "src": "/(.*)", "dest": "build/$1" }
  ],
  "builds": [
    { "src": "server/index.js", "use": "@vercel/node" },
    { "src": "package.json", "use": "@vercel/static-build" }
  ]
}
